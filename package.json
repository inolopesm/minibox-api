  "scripts": {
    "sls:offline": "serverless offline",
    "sls:create_domain": "serverless create_domain",
    "sls:delete_domain": "serverless delete_domain",
    "sls:deploy": "serverless deploy",
    "sls:deploy:production": "serverless deploy --stage production",
    "test": "jest",
    "lint": "eslint **/*.ts",
    "format": "prettier **/*.ts --write",
    "knex": "node -r dotenv/config -r ts-node/register node_modules/knex/bin/cli --knexfile knexfile.js",
